    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-resizable": "^3.0.5",
    "@vitest/ui": "^0.34.0",
    "antd": "^5.3.0",
    "autoprefixer": "^10.4.17",
    "bumpp": "^9.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.52.0",
    "fs-extra": "^11.1.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0",
    "terser": "^5.24.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.1.4",
    "vitest": "^0.34.0"
  }
