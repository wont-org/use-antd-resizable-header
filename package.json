    "react-resizable": "^3.0.5",
    "react-resize-detector": "^8.0.3"
  },
  "devDependencies": {
    "@ant-design/pro-table": "^3.13.11",
    "@minko-fe/eslint-config": "^2.0.5",
    "@minko-fe/tsconfig": "^2.0.5",
    "@testing-library/dom": "^8.20.0",
