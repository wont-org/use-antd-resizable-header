{
  "name": "@wont/use-antd-resizable-header",
  "version": "1.0.1",
  "description": "antd resizable header column hook",
  "type": "module",
  "keywords": [
    "react",
    "antd",
    "resiable header"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wont-org/use-antd-resizable-header",
